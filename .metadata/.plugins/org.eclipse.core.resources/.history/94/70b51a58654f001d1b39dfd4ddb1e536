package sample;

public class UnaryOperators {
	public static void main(String[] args) {
		int num = 100;
		int result
		// Increment
		num++;
		System.out.println(num);
		num--;
		System.out.println(num);
		
		// Decrement
		num--;
		System.out.println(num);
		--num;
		System.out.println(num);
	}
}
